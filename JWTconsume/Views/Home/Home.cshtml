@using System.Data;
@model Ragistered;
<div class="main-panel">
    

    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-striped">
                        <thead>
                            
                            <tr>
                                
                                <th> @Html.DisplayName("Id") </th>
                                <th> @Html.DisplayName("User") </th>
                                <th> @Html.DisplayName("Email") </th>
                                
                            </tr>
                        </thead>
                        <tbody>
                            
                           <tr>
                               <th>@Html.DisplayFor(model=>model.Id)</th>
                                <th>@Html.DisplayFor(model=>model.username)</th>
                                <th>@Html.DisplayFor(model=>model.email)</th>
                           </tr>
                           
                            
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

